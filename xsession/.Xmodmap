clear lock
! caps -> escape
keycode 66 = Escape NoSymbol NoSymbol
! menu -> super
keycode 135 = Super_L NoSymbol NoSymbol

! right to left, starting from F12; only need this
! for keyboards without dedicated media keys
!
! keycode 96 = XF86AudioRaiseVolume
! keycode 95 = XF86AudioLowerVolume
! keycode 76 = XF86AudioMute
! keycode 75 = XF86AudioPlay
!
! for the MS 4000 Ergonomic KB
keycode 195 = XF86AudioPrev
keycode 196 = XF86AudioNext
