[user]
  name = Victor Alvarez
  email = victor.alvarez@woven-planet.global
  ; email = sapien.reversing.entropy@gmail.com
[core]
  editor = nvim
  excludesfile = /Users/victor.alvarez/.gitignore
[alias]
  co = checkout
  br = branch
  ci = commit
  st = status
  ph = push
  pl = pull
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[push]
  autoSetupRemote = true
[merge]
  conflictStyle = zdiff3
[diff]
  external = difft --display side-by-side
