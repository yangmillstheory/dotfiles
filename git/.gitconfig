[user]
  name = Victor Alvarez
  email = sapien.reversing.entropy@gmail.com
[core]
  editor = nvim
  pager = delta
  excludesfile = /Users/yangmillstheory/.gitignore
[alias]
  co = checkout
  br = branch
  ci = commit
  st = status
  ph = push
  pl = pull
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[push]
  autoSetupRemote = true
[interactive]
  diffFilter = delta --color-only --side-by-side
[delta]
  navigate = true
  dark = true
[merge]
  conflictStyle = zdiff3
