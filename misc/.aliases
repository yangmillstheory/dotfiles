##################
# misc shell stuff
alias v='nvim'
alias o='xdg-open'
alias q='exit'
alias c='/usr/bin/clear'
alias x='/bin/systemctl suspend'
alias irc='/usr/bin/irssi -c google'
alias sudo='nocorrect /usr/bin/sudo'

#################
# 3rd party stuff
# tmux
alias t='tmx2'
alias ta='tmx2 attach-session -t'
alias tn='/usr/bin/tmx2 new-session -s'
alias fd=fdfind

##############
# google stuff
alias paste='/google/src/head/depot/eng/tools/pastebin'
alias dingo='/google/data/ro/projects/production/tools/dingo'

# fig
alias h='nocorrect /usr/bin/chg'
alias hf='nocorrect /usr/bin/chg fix -w'
alias hu='nocorrect /usr/bin/chg uc'
alias ha='nocorrect /usr/bin/chg amend'
alias hi='hf; ha; hu'
alias he='nocorrect /usr/bin/chg amend --edit'
alias hc='nocorrect /usr/bin/chg ci'
alias hv='nocorrect /usr/bin/chg evolve'
alias hs='nocorrect /usr/bin/chg status'
alias hy='nocorrect /usr/bin/chg sync'
alias hp='nocorrect /usr/bin/chg presubmit -d'
alias hx='nocorrect /usr/bin/chg x'
alias hz='nocorrect /usr/bin/chg diff --pager=1'

# blaze
alias blaze='nocorrect /usr/bin/blaze'
alias bl='blaze'
alias blb='blaze build'
alias blt='blaze test'
alias blr='blaze run'
alias cb='build_cleaner'

# pubsub
alias pubsub2cfg="/google/data/ro/projects/goops/pubsub2cfg" # go/pubsub2cfg
alias pubsub="/google/data/ro/projects/goops/pubsub" # go/pubsub-tool
alias goops_proxy="/google/data/ro/projects/goops/goops_proxy" # go/goops_proxy
alias cps_proxy="/google/data/ro/teams/cloud-pubsub/cps_proxy" # go/cps_proxy
alias cpsadmin="/google/data/ro/teams/cloud-pubsub/cpsadmin" # go/cpsadmin
alias cps_span="/google/data/ro/teams/cloud-pubsub/cps_span" # go/cps_span
alias admin_session="/google/data/ro/projects/tonic/admin_session" # go/admin_session
alias onborg="/google/data/ro/projects/smartass/onborg" # go/onborg
alias annealing="/google/bin/releases/rollouts/annealing/annealing" # go/annealing-cli
alias prodspec="/google/bin/releases/rollouts/prodspec/prodspec" # go/prodspec-cli
alias cpl="cpsadmin lookup"
alias p="gcert"
